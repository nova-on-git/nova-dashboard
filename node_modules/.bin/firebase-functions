#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cody/git/veloris-dashboard/node_modules/.pnpm/firebase-functions@6.0.1_firebase-admin@12.6.0/node_modules/firebase-functions/lib/bin/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/firebase-functions@6.0.1_firebase-admin@12.6.0/node_modules/firebase-functions/lib/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/firebase-functions@6.0.1_firebase-admin@12.6.0/node_modules/firebase-functions/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/firebase-functions@6.0.1_firebase-admin@12.6.0/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cody/git/veloris-dashboard/node_modules/.pnpm/firebase-functions@6.0.1_firebase-admin@12.6.0/node_modules/firebase-functions/lib/bin/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/firebase-functions@6.0.1_firebase-admin@12.6.0/node_modules/firebase-functions/lib/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/firebase-functions@6.0.1_firebase-admin@12.6.0/node_modules/firebase-functions/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/firebase-functions@6.0.1_firebase-admin@12.6.0/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../firebase-functions/lib/bin/firebase-functions.js" "$@"
else
  exec node  "$basedir/../firebase-functions/lib/bin/firebase-functions.js" "$@"
fi
