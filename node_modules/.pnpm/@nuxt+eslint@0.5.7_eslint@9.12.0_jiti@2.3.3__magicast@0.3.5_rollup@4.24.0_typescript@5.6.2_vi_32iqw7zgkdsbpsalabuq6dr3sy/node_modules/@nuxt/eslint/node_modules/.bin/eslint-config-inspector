#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cody/git/veloris-dashboard/node_modules/.pnpm/@eslint+config-inspector@0.5.4_eslint@9.12.0_jiti@2.3.3_/node_modules/@eslint/config-inspector/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/@eslint+config-inspector@0.5.4_eslint@9.12.0_jiti@2.3.3_/node_modules/@eslint/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/@eslint+config-inspector@0.5.4_eslint@9.12.0_jiti@2.3.3_/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cody/git/veloris-dashboard/node_modules/.pnpm/@eslint+config-inspector@0.5.4_eslint@9.12.0_jiti@2.3.3_/node_modules/@eslint/config-inspector/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/@eslint+config-inspector@0.5.4_eslint@9.12.0_jiti@2.3.3_/node_modules/@eslint/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/@eslint+config-inspector@0.5.4_eslint@9.12.0_jiti@2.3.3_/node_modules:/home/cody/git/veloris-dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@eslint+config-inspector@0.5.4_eslint@9.12.0_jiti@2.3.3_/node_modules/@eslint/config-inspector/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../../@eslint+config-inspector@0.5.4_eslint@9.12.0_jiti@2.3.3_/node_modules/@eslint/config-inspector/bin.mjs" "$@"
fi
